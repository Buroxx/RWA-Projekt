  create procedure CreateNewUser
  @username nvarchar(50),
  @email nvarchar(100),
  @password nvarchar(max),
  @phone nvarchar(max),
  @address nvarchar(max)
  as
  insert into AspNetUsers(UserName, Email, EmailConfirmed, PasswordHash, PhoneNumber, PhoneNumberConfirmed, Address, AccessFailedCount, LockoutEnabled)
  values (@username, @email, 0, @password, @phone, 0, @address, 0, 0)
  go
